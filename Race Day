let raceNumber = Math.floor(Math.random() * 1000);
let registeredEarly = false;
let runnerAge = '19';
 if (runnerAge > '18' && registeredEarly)
  { raceNumber +=1000; } 
/*else {console.log(raceNumber);
}*/

  if (runnerAge >'18' && registeredEarly ) {
  console.log(`You will race at 9:30 am and your raceNumber is ${raceNumber}`);


} else if ( runnerAge >'18' && !registeredEarly) {
  console.log(`You will race at 11:00 am and your raceNumber is ${raceNumber}`); }

else if (runnerAge <'18') {console.log(`You will race at 12:30 pm and your raceNumber is  ${raceNumber}`);
